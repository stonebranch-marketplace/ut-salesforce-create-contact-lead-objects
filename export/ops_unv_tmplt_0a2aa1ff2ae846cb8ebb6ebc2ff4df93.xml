###################################################
#
#    Name: SalesForce.py
#
#    Purpose: using simple_salesforce to automate SalesForce
#
#    Prerequisites:
#     - Universal Agent for Windows/Linux
#     - Python 3.x
#     - simple_salesforce module
#
#
#     Version History:
#     1.0  Moritz Roos   2020-04-01  initial script
#
###################################################

# Used modules
import logging
import subprocess
import os
from simple_salesforce import Salesforce
from simple_salesforce import SalesforceError
import requests
from pprint import pformat

version = "1.0"
logging.basicConfig(level="DEBUG", format=' %(asctime)s - %(levelname)s - %(message)s')
#logging.basicConfig(level=logging.${ops_var_loglevel}, format = ' %(asctime)s - %(levelname)s - %(message)s')

def execute_SOQL(sf):
    logging.debug('FUNC: execute_SOQL')

    try:
        with open(r'''${_scriptPath('${ops_sf_soql}')}''', 'r') as script:
            query = script.read()
        output = sf.query_all(query)
        #logging.info("Returned from command: " + str(output).replace("\\r\\n", "\n"))
        logging.info("Returned from command: " + pformat(output))
    except SalesforceError as e:
        logging.error("Returned from command: " + str(e.content).replace("\\r\\n", "\n"))
        logging.error("Error Code: " + str(e.status))
        exit(e.status)


def create_contact(sf):
    logging.debug('FUNC: create_contact')
    firstname = "${ops_sf_firstname}"
    lastname = "${ops_sf_lastname}"
    email = "${ops_sf_email}"
    try:
        output = sf.Contact.create({'LastName': lastname, 'Email': email, 'FirstName': firstname})
        logging.info("Returned from command: " + str(output).replace("\\r\\n", "\n"))
    except SalesforceError as e:
        logging.error("Returned from command: " + str(e.content).replace("\\r\\n", "\n"))
        logging.error("Error Code: " + str(e.status))
        exit(e.status)

def open_session(username, password, security_token):
    logging.debug('FUNC: open_session')
    session = requests.Session()
    try:
        sf = Salesforce(username=username, password=password, security_token=security_token, session=session)
        logging.info("Returned from command: " + str(sf).replace("\\r\\n", "\n"))
    except SalesforceError as e:
        logging.error("Returned from command: " + str(e.message).replace("\\r\\n", "\n"))
        logging.error("Error Code: " + str(e.code))
        exit(e.code)
    return sf

def set_variables():
    logging.info("Executing version " + version)

def main():
    set_variables()
    function = "${ops_sf_function}"
    username = "${_credentialUser('${ops_sf_credential}')}"
    password = "${_credentialPwd('${ops_sf_credential}')}"
    security_token = "${_credentialPassphrase('${ops_sf_credential}')}"
    sf = open_session(username, password, security_token)
    if function == "Create Contact":
        create_contact(sf)
    if function == "Execute SOQL":
        execute_SOQL(sf)

# -- Execute
if __name__ == '__main__':
    main()
